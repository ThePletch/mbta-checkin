<h3>{{stop_name}}</h3>
<ul>
{{#each mode}}
    {{#each route}}
        <h5>{{route_name}}</h5>
        <div class="prediction-well">
        {{#each direction}}
            <div class="prediction-well">
                {{direction_name}}: {{predict_str}} ({{away_str}})
                <div class="prediction-well">
                    <ul>
                        <li>{{time_between_stops}} between {{vehicle_name}}</li>
                    </ul>
                </div>
            </div>
        {{/each}}
        </div>
    {{/each}}
{{/each}}
</ul>